parserOptions:
  ecmaVersion: 9
  ecmaFeatures:
    modules: true
    jsx: true

env:
  amd: true
  browser: true
  es6: true
  jquery: true
  node: true

globals:
  wp: true
  ajaxurl: true
  gofer_seo_l10n_data: true
  gofer_seo_screen_module_debugger_l10n: true
  gofer_seo_l10n_post_editor_general: true
  gofer_seo_l10n_add_field_list: true
  gofer_seo_l10n_add_field_robots_txt: true
  gofer_seo_l10n_snippet: true
  gofer_seo_notice_data: true

# http://eslint.org/docs/rules/
# 0 = disabled
# 1 = warning
# 2 = error
rules:
  # Possible Problems
  array-callback-return: 0
  constructor-super: 2
  for-direction: 2
  getter-return: 2
  no-async-promise-executor: 2
  no-await-in-loop: 0
  no-class-assign: 2
  no-compare-neg-zero: 2
  no-cond-assign: 2
  no-const-assign: 2
  no-constant-condition: 2
  no-constructor-return: 0
  no-control-regex: 2
  no-debugger: 2
  no-dupe-args: 2
  no-dupe-class-members: 2
  no-dupe-else-if: 2
  no-dupe-keys: 2
  no-duplicate-case: 2
  no-duplicate-imports: 0
  no-empty-character-class: 2
  no-empty-pattern: 2
  no-ex-assign: 2
  no-fallthrough: 2
  no-func-assign: 2
  no-import-assign: 2
  no-inner-declarations: [ 2, functions ]
  no-invalid-regexp: 2
  no-irregular-whitespace: 2
  no-loss-of-precision: 2
  no-misleading-character-class: 2
  no-new-symbol: 2
  no-obj-calls: 2
  no-promise-executor-return: 0
  no-prototype-builtins: 2
  no-self-assign: 2
  no-self-compare: 0
  no-setter-return: 2
  no-sparse-arrays: 2
  no-template-curly-in-string: 0
  no-this-before-super: 2
  no-undef: 2
  no-unexpected-multiline: 2
  no-unmodified-loop-condition: 0
  no-unreachable: 2
  no-unreachable-loop: 0
  no-unsafe-finally: 2
  no-unsafe-negation: 2
  no-unsafe-optional-chaining: 2
  no-unused-private-class-members: 0
  no-unused-vars: [ 2, { args: none } ]
  no-use-before-define: 0
  no-useless-backreference: 2
  require-atomic-updates: 0
  use-isnan: 2
  valid-typeof: 2

  # Suggestions
  accessor-pairs: 2
  arrow-body-style: 0
  block-scoped-var: 0
  camelcase: 0
  capitalized-comments: 0
  class-methods-use-this: 0
  complexity: [ 2, 24 ]
  consistent-return: 0
  consistent-this: 0
  curly: 0
  default-case: 0
  default-case-last: 0
  default-param-last: 0
  dot-notation: 0
  eqeqeq: 0
  func-name-matching: 0
  func-names: 0
  func-style: 0
  grouped-accessor-pairs: 0
  guard-for-in: 0
  id-denylist: 0
  id-length: 0
  id-match: 0
  init-declarations: 0
  max-classes-per-file: 0
  max-depth: 0
  max-lines: 0
  max-lines-per-function: 0
  max-nested-callbacks: 0
  max-params: 0
  max-statements: [ 2, 180 ]
  multiline-comment-style: 0
  new-cap: 0
  no-alert: 0
  no-array-constructor: 0
  no-bitwise: 0
  no-caller: 0
  no-case-declarations: 2
  no-confusing-arrow: 0
  no-console: 0
  no-continue: 0
  no-delete-var: 2
  no-div-regex: 0
  no-else-return: 0
  no-empty: 2
  no-empty-function: 0
  no-eq-null: 0
  no-eval: 0
  no-extend-native: 0
  no-extra-bind: 0
  no-extra-boolean-cast: 2
  no-extra-label: 0
  no-extra-semi: 2
  no-floating-decimal: 0
  no-global-assign: 2
  no-implicit-coercion: 0
  no-implicit-globals: 0
  no-implied-eval: 0
  no-inline-comments: 0
  no-invalid-this: 0
  no-iterator: 0
  no-label-var: 0
  no-labels: 0
  no-lone-blocks: 0
  no-lonely-if: 0
  no-loop-func: 0
  no-magic-numbers: 0
  no-mixed-operators: 0
  no-multi-assign: 0
  no-multi-str: 0
  no-negated-condition: 0
  no-nested-ternary: 0
  no-new: 0
  no-new-func: 0
  no-new-object: 0
  no-new-wrappers: 0
  no-nonoctal-decimal-escape: 2
  no-octal: 2
  no-octal-escape: 0
  no-param-reassign: 0
  no-plusplus: 0
  no-proto: 0
  no-redeclare: 2
  no-regex-spaces: 2
  no-restricted-exports: 0
  no-restricted-globals: 0
  no-restricted-imports: 0
  no-restricted-properties: 0
  no-restricted-syntax: 0
  no-return-assign: 0
  no-return-await: 0
  no-script-url: 0
  no-sequences: 0
  no-shadow: 0
  no-shadow-restricted-names: 2
  no-ternary: 0
  no-throw-literal: 0
  no-undef-init: 0
  no-undefined: 0
  no-underscore-dangle: 0
  no-unneeded-ternary: 0
  no-unused-expressions: 0
  no-unused-labels: 2
  no-useless-call: 0
  no-useless-catch: 2
  no-useless-computed-key: 0
  no-useless-concat: 0
  no-useless-constructor: 0
  no-useless-escape: 2
  no-useless-rename: 0
  no-useless-return: 0
  no-var: 0
  no-void: 0
  no-warning-comments: 0
  no-with: 2
  object-shorthand: 0
  one-var: 0
  one-var-declaration-per-line: 0
  operator-assignment: 0
  prefer-arrow-callback: 0
  prefer-const: 0
  prefer-destructuring: 0
  prefer-exponentiation-operator: 0
  prefer-named-capture-group: 0
  prefer-numeric-literals: 0
  prefer-object-spread: 0
  prefer-promise-reject-errors: 0
  prefer-regex-literals: 0
  prefer-rest-params: 0
  prefer-spread: 0
  prefer-template: 0
  quote-props: 0
  radix: 0
  require-await: 0
  require-unicode-regexp: 0
  require-yield: 2
  sort-imports: 0
  sort-keys: 0
  sort-vars: 0
  spaced-comment: 0
  strict: 0
  symbol-description: 0
  vars-on-top: 0
  yoda: 0

  # Layout & Formatting.
  array-bracket-newline: 0
  array-bracket-spacing: 0
  array-element-newline: 0
  arrow-parens: 0
  arrow-spacing: 0
  block-spacing: 0
  brace-style: 0
  comma-dangle: [ 2, never ]
  comma-spacing: 0
  comma-style: 0
  computed-property-spacing: 0
  dot-location: 0
  eol-last: 0
  func-call-spacing: 0
  function-call-argument-newline: 0
  function-paren-newline: 0
  generator-star-spacing: 0
  implicit-arrow-linebreak: 0
  indent: 0
  jsx-quotes: 0
  key-spacing: 0
  keyword-spacing: 0
  line-comment-position: 0
  linebreak-style: 0
  lines-around-comment: 0
  lines-between-class-members: 0
  max-len: 0
  max-statements-per-line: 0
  multiline-ternary: 0
  new-parens: 0
  newline-per-chained-call: 0
  no-extra-parens: 0
  no-mixed-spaces-and-tabs: 2
  no-multi-spaces: 0
  no-multiple-empty-lines: 0
  no-tabs: 0
  no-trailing-spaces: 0
  no-whitespace-before-property: 0
  nonblock-statement-body-position: 0
  object-curly-newline: 0
  object-curly-spacing: 0
  object-property-newline: 0
  operator-linebreak: 0
  padded-blocks: 0
  padding-line-between-statements: 0
  quotes: 0
  rest-spread-spacing: 0
  semi: 0
  semi-spacing: 0
  semi-style: 0
  space-before-blocks: 0
  space-before-function-paren: 0
  space-in-parens: 0
  space-infix-ops: 0
  space-unary-ops: 0
  switch-colon-spacing: 0
  template-curly-spacing: 0
  template-tag-spacing: 0
  unicode-bom: 0
  wrap-iife: 0
  wrap-regex: 0
  yield-star-spacing: 0
