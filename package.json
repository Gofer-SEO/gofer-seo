{
	"name": "gofer-seo",
	"version": "1.0.0",
	"description": "SEO plugin to Gofer.",
	"main": "gofer-seo.php",
	"directories": {
		"test": "tests"
	},
	"scripts": {
		"grunt": "grunt",
		"sass:build": "sass gofer-seo/admin/css/main.scss:gofer-seo/admin/css/main.css",
		"sass:watch": "sass --watch gofer-seo/admin/css/main.scss:gofer-seo/admin/css/main.css",
		"sass:watch:compress": "sass --watch gofer-seo/admin/css/main.scss:gofer-seo/admin/css/main.css --style compressed",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses --prod --gpl2",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:md": "wp-scripts lint-md",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"watch": "wp-scripts build --config webpack-config.js --watch",
		"build": "wp-scripts build --config webpack-config.js",
		"packages-update": "wp-scripts packages-update",
		"postpackages-update": "npm install",
		"plugin-zip": "wp-scripts plugin-zip"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Gofer-SEO/gofer-seo.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/Gofer-SEO/gofer-seo/issues"
	},
	"homepage": "https://github.com/Gofer-SEO/gofer-seo#readme",
	"devDependencies": {
		"@wordpress/icons": "^6.1.1",
		"@wordpress/scripts": "^19.2.3",
		"@wordpress/stylelint-config": "^19.1.0",
		"classnames": "^2.2.6",
		"dom-scroll-into-view": "^2.0.1",
		"grunt": "^1.3.0",
		"grunt-contrib-csslint": "^2.0.0",
		"grunt-contrib-cssmin": "^3.0.0",
		"grunt-contrib-jshint": "^3.0.0",
		"grunt-contrib-uglify": "^5.0.0",
		"grunt-eslint": "^23.0.0",
		"grunt-mkdir": "^1.1.0",
		"grunt-phpcbf": "^0.1.1",
		"grunt-phpcs": "^0.4.0",
		"grunt-phplint": "^0.1.0",
		"grunt-sass": "~3.1.0",
		"grunt-stylelint": "^0.17.0",
		"lodash": "^4.17.15",
		"node-sass": "^7.0.1",
		"sass": "^1.49.0"
	}
}
